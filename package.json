{
  "name": "AgendeiAqui",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "db:studio": "npx prisma studio",
    "db:seed": "node scripts/seed.js",
    "db:start:dev": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:start:prod": "npm run db:generate && npm run db:deploy && npm run db:seed",
    "start": "node src/app.js",
    "start:prod": "node -r bytenode dist/app.jsc",
    "test": "nodemon src/app.js",
    "watch:tailwind": "npx tailwindcss -i ./src/public/css/tailwind/style.css -o ./src/public/css/tailwind.css --watch",
    "dev": "concurrently \"npm run test\" \"npm run watch:tailwind\"",
    "start:dev": "concurrently \"npm run start\" \"npm run watch:tailwind\"",
    "build": "node scripts/compile.js",
    "build:prod": "docker run --rm -v \"%cd%\":/app -w /app node:22 npm run build",
    "publish:prod": "node scripts/publish_prod.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bytenode": "^1.5.7",
    "concurrently": "^8.2.2",
    "javascript-obfuscator": "latest",
    "morgan": "^1.10.0",
    "prisma": "^6.4.1",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "@prisma/client": "^6.4.1",
    "accounting": "^0.4.1",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.17.3",
    "express-subdomain-handler": "^0.1.0",
    "luxon": "^3.5.0",
    "mercadopago": "^2.3.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.47",
    "node-cron": "^3.0.3",
    "nodemon": "^2.0.22",
    "pix-utils": "^2.6.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "uuid": "^11.0.5"
  }
